$banner-name = 'banner-' + data.main.dimensions.width + 'x' + + data.main.dimensions.height

// Font-family
$fontDefault = unquote(data.main.style.defaultFont)

// Colors
$color-blue = unquote(data.main.style.color1)
$color-gray = unquote(data.main.style.color2)
$color-green = unquote(data.main.style.color3)
$color-dark = unquote(data.main.style.color4)

// Offset
$width = unit(data.main.dimensions.width - 2, 'px')
$heigth = unit(data.main.dimensions.height - 2, 'px')
$image-height = unit(data.main.dimensions.imageHeight, 'px')
$image-width = unit(data.main.dimensions.imageWidth, 'px')
$content-padding = unit(data.main.dimensions.contentPadding, 'px')
